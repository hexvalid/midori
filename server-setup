az network nic update -g RG -n midori-server795 --ip-forwarding true

for i in {28..64}
do
   az network public-ip create -g RG --sku Basic --version IPv4 -n IP-$i --allocation-method Dynamic --idle-timeout 30 --location westeurope
   az network nic ip-config create -g RG  --nic-name midori-server795 --name ic-$i --private-ip-address 10.0.1.$i --private-ip-address-version IPv4 --public-ip-address IP-$i
   echo "sleeping..."
   sleep $[ ( $RANDOM % 300 )  + 1 ]s
done



